#include <stdio.h>

int main(void) {
  
  int X = 0, Y = 0, intervalo = 0, resto = 0, cont = 0, divisor = 1;
  
  
  printf("\n - Digite o valor de X:"
         "\n  --> ");
  scanf("%i", &X);
  printf("\n - Digite o valor de Y:"
         "\n  --> ");
  scanf("%i", &Y);

  if (X > Y) {
    
    for (cont = Y; cont <= X; cont++) { 
      divisor = 1;
      printf("\n  --> divisores de %i sao:", cont);
      while (divisor <= cont) {
        
        if (cont % divisor == 0) {
          if (divisor == 1){
            printf("\n   --> %i", divisor);
          } else {
          printf(", %i", divisor);
          }
        }
        divisor++;
      }
      cont++;
    }
  } else  if (Y > X) {
     
    for (cont = X; cont <= Y; cont++) {
      divisor = 1;
      printf("\n  --> divisores de %i sao:", cont);
      while (divisor <= cont) {
        
        if (cont % divisor == 0) {
          if (divisor == 1){
            printf("\n   --> %i", divisor);
          } else {
          printf(", %i", divisor);
          }
        }
        divisor++;
      }
      cont++;
    }
  } else {
    printf("\n - Os numeros sao iguais portando nao existe intervalo.");
  }
  
  return 0;
}
